{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["**/.env.*local", "**/.env"],
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**", "dist/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "start": {
        "dependsOn": ["build"]
      },
      "lint": {},
      "type-check": {},
      "db:generate": {
        "cache": false
      },
      "db:push": {
        "cache": false
      },
      "db:migrate": {
        "cache": false
      }
    }
  }
  